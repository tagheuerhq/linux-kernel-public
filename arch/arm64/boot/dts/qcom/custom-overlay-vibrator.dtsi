		qcom,wf-play-rate-us = <4180>;
	};
	wf_4 {
		/* POP */
		qcom,effect-id = <4>;
		qcom,wf-vmax-mv = <2680>;
		qcom,wf-pattern = [7e 7e];
		qcom,wf-play-rate-us = <4180>;
	};
	wf_5 {
		/* HEAVY CLICK */
		qcom,effect-id = <5>;
		qcom,wf-vmax-mv = <2680>;
		qcom,wf-pattern = [7e 7e 7e];
		qcom,wf-play-rate-us = <4180>;
	};
